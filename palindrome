import java.util.*;
class abcd
{
public static void main(String args[])
{
int n,temp,rem,rev=0;
Scanner sc=new Scanner(System.in);
System.out.println("enter the number");
n=sc.nextInt();
temp=n;
while(n>0)
{
rem=n%10;
rev=(rev*10)+rem;
n=n/10;
}
if (temp==rev){
System.out.println("is a palindrome");
}
else
{
System.out.println("not a palindrome");
}
}
}
